<?xml version="1.0" encoding="UTF-8"?>
<cxp:make xmlns:cxp="http://www.tenbusch.info/cxproc">
  <cxp:subst string="%CACHEDIR%" env="CXP_CACHE" default="Cache"/>
  <cxp:subst string="%CGIPATH%" cgi="path"/>
  <cxp:subst string="%CGIDIR%" dir="%CGIPATH%"/>
  <!-- TODO: check value of 'path' -->
  <cxp:plain name="-" response="OK">Move '%CGIPATH%' to '%CACHEDIR%/%CGIDIR%' </cxp:plain>
  <cxp:copy from="%CGIPATH%" to="%CACHEDIR%/%CGIDIR%/" delete="yes"/>
</cxp:make>
